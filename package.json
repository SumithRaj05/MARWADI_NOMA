{
  "name": "sambhav-server",
  "version": "1.0.0",
  "description": "MERN Stack Finance Management Application",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm run dev",
